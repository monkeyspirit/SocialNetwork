      <Tab closable="false" text="Bacheca">
         <content>
            <SplitPane disable="true" dividerPositions="0.3">
              <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" pickOnBounds="false" prefHeight="160.0" prefWidth="100.0">
                     <children>
                        <ListView fx:id="categoryListView" prefHeight="200.0" prefWidth="158.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                <AnchorPane minHeight="200.0" minWidth="158.0" prefHeight="160.0" prefWidth="100.0">
                     <children>
                        <ListView fx:id="eventListView" prefHeight="400.0" prefWidth="416.0" />
                     </children>
                  </AnchorPane>
              </items>
            </SplitPane>
         </content>
      </Tab>
      <Tab text="Utente">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
   </tabs>
</TabPane>
